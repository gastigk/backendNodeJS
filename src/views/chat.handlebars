<link
  rel="stylesheet"
  href="//cdn.jsdelivr.net/npm/sweetalert2@11.1.3/dist/sweetalert2.min.css"
/>
<section
  class="col d-flex flex-column justify-content-center align-items-center michat"
>
  <div class="container card-chat">
    <div class="col-sm-6 col-sm-offset-3 form-box">
      {{#if user}}
        <h3 id="UserNameChat">Wellcome
          {{user.first_name}}
          {{user.last_name}}
          {{user.user.first_name}}
          al Chat de El SÃ©ptimo Rayo</h3>
        <p>
          User e-mail:
        </p>
        <p id="username">
          {{user.email}}
          {{user.user.email}}
        </p>
      {{/if}}
      <input
        type="text"
        id="chatBox"
        class="form-control"
        placeholder="Write a message"
      />
      <hr />
      <h3>Message History</h3>
      <div id="history"></div>

    </div>
  </div>
</section>

<script
  src="https://cdn.jsdelivr.net/npm/sweetalert2@11.1.3/dist/sweetalert2.min.js"
></script>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
