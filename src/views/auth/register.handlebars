                                <form class="auth-register-form mt-2" action="/auth/register" method="POST">
                                    <div class="mb-1">
                                        <label class="form-label" for="first_name">First Name</label>
                                        <input class="form-control" id="first_name" type="text" name="first_name" placeholder="John" aria-describedby="register-username" autofocus="" tabindex="1" required/>
                                    </div>
                                    <div class="mb-1">
                                        <label class="form-label" for="last_name">Last Name</label>
                                        <input class="form-control" id="last_name" type="text" name="last_name" placeholder="Sánchez" aria-describedby="register-username" autofocus="" tabindex="2" required/>
                                    </div>
                                    <div class="mb-1">
                                        <label class="form-label" for="email">Email</label>
                                        <input class="form-control" id="email" type="text" name="email" placeholder="johnsanchez@example.com" aria-describedby="register-email" tabindex="3" required/>
