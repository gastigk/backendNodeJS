    <div class="app-content content ">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper container-xxl p-0">
            <div class="content-header row">
                <div class="content-header-left col-md-9 col-12 mb-2">
                    <div class="row breadcrumbs-top">
                        <div class="col-12">
                            <h2 class="content-header-title float-start mb-0">Cart</h2>
                            <div class="breadcrumb-wrapper">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/">Home</a>
                                    </li>
                                    <li class="breadcrumb-item"><a href="/products">Shop</a>
                                    </li>
                                    <li class="breadcrumb-item active">My Cart
                                    </li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="content-body">
                <!-- Basic table -->
                <section id="basic-datatable">
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <table class="datatables-basic table">
                                    <thead>
                                        <tr>
                                            <th>Product</th>
                                            <th>Price
                                                <form action="/cart" method="get" id="sortForm">
                                                    <select name="sortOption" onchange="document.getElementById('sortForm').submit()">
                                                        <option value="">Filtrar</option>
                                                        <option value="asc">Menor Precio</option>
                                                        <option value="desc">Mayor Precio</option>
                                                    </select>
                                                    <input type="hidden" name="sort" value="{{ sortOption }}">
                                                </form>
                                            </th>
                                            <th>Photo</th>
                                            <th>Quantity</th>
                                            <th>Total</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each cart.items as |item|}}
                                        <tr>
                                            <td>{{ item.producto.title }}</td>
                                            <td>$ {{ item.producto.price }} </td>
                                            <td style="width: 10rem;">
                                                <img src="{{ item.producto.thumbnail }}" class="figure-img img-fluid rounded" alt="{{ item.producto.title }}">
                                            </td>

                                            <td>{{this.phone}}</td>
                                            <td>
                                                <form method="POST" action="/cart/{{ ../cid }}/{{item._id}}?_method=PUT"
                                                    style="display: flex; justify-content: space-evenly;" class="input-con-botones">
                                                    <input type="hidden" name="cid" value="{{ cid }}">
                                                    <input type="number" value="{{ item.cantidad }}" class="form-control input-width"
                                                        name="cantidad" min="1" max="{{ item.producto.stock }}" style="max-height: 2rem;">
                                                    <button type="submit" class="btn btn-primary">
                                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                                            class="bi bi-check-lg" viewBox="0 0 16 16">
                                                            <path
                                                                d="M12.736 3.97a.733.733 0 0 1 1.047 0c.286.289.29.756.01 1.05L7.88 12.01a.733.733 0 0 1-1.065.02L3.217 8.384a.757.757 0 0 1 0-1.06.733.733 0 0 1 1.047 0l3.052 3.093 5.4-6.425a.247.247 0 0 1 .02-.022Z" />
                                                        </svg>
                                                    </button>
                                                </form>
                                            </td>
                                            <td>${{ multiply item.producto.price item.cantidad }}</td>
                                            <td>
                                                <a href="/notifications/deleted-cart/{{ ../cid }}/{{ item._id }}" class="btn btn-danger input-con-botones enlaces">
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-cart-x" viewBox="0 0 16 16">
                                                        <path d="M7.354 5.646a.5.5 0 1 0-.708.708L7.793 7.5 6.646 8.646a.5.5 0 1 0 .708.708L8.5 8.207l1.146 1.147a.5.5 0 0 0 .708-.708L9.207 7.5l1.147-1.146a.5.5 0 0 0-.708-.708L8.5 6.793 7.354 5.646z" />
                                                        <path d="M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1H.5zm3.915 10L3.102 4h10.796l-1.313 7h-8.17zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0z" />
                                                    </svg>
                                                </a>
                                            </td>
                                        </tr>
                                        {{/each}}
                                        {{#if userIsPremium}}
                                        <div class="apply-discount">
                                            <h1>20% off!</h1>
                                        </div>
                                        {{/if}}
                                        <tr class="totalPrice">
                                            {{#if user.premium}}
                                            <td colspan="6">
                                                {{#with totalPrice}}
                                                TOTAL: ${{this}}.- <br>(20% off)
                                                {{/with}}
                                            </td>
                                            {{else}}
                                            <td colspan="6">
                                                {{#with totalPrice}}
                                                TOTAL: ${{this}}.-
                                                {{/with}}
                                            </td>
                                            {{/if}}
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
                <!--/ Basic table -->

                <!-- Buttoms -->            
                <div class="btn-group" role="group" aria-label="Basic radio toggle button group">
                    <button type="submit" class="btn btn-primary btn-cart" checked id="comprar-carro">                                    
                        <i data-feather="shopping-cart"></i>
                        <span class="add-to-cart">Buy cart</span>
                    </button>                    
                    <form method="POST" action="/cart/{{ cid }}/clear" id="emptyCartForm"></form>
                        <input type="hidden" name="cid" value="{{ cid }}" class="btn-check" id="emptyCartForm" autocomplete="off">
                        <button type="submit" class="btn btn-secondary btn-cart" id="vaciar-carro">                                    
                            <span class="add-to-cart">Clear cart</span>
                        </button>
                    </form>
                </div>
                <div class="btn mt-1">
                    <form method="POST" action="/cart/{{ cid }}/delete" id="cid">
                        <input type="hidden" name="cid" value="{{ cid }}" class="btn-check" id="cid" autocomplete="off">
                        <button type="submit" class="btn btn-secondary btn-cart" id="eliminar-carro">                                    
                            <span class="add-to-cart">Delete cart</span>
                        </button>                    
                    </form>
                </div>
                <!--/ Buttoms -->

                <!-- Cheackout -->    
                <section id="checkout-section" class="hidden">
                    <h1 class="premium-h1">Checkout</h1>
                    <div class="card-container">
                        <div class="card-form">
                            <input type="number" class="form-control mb-2" placeholder="Número de tarjeta" id="card-number"
                                name="cardNumber" min="0" max="9999999999999999" required>
                            <input type="text" class="form-control mb-2" placeholder="Nombre y Apellido del Titular" id="card-name"
                                name="cardName" required>
                            <input type="number" class="form-control mb-2" placeholder="MM/AA" id="card-expiration"
                                name="cardExpiration" min="0" max="1300" required>
                            <input type="number" class="form-control mb-2" placeholder="Código de Seguridad" id="card-cvv"
                                name="cardCvv" min="0" max="999" required>
                            <form action="/checkout" method="POST">
                                <input type="hidden" name="cid" value="{{ cid }}">
                                <div class="input-group group-comprar">
                                    <button type="submit" class="btn btn-success" id="pagar-carro" disabled>Pay</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
